        "PluginFormat": 2
      },
      {
        "PluginName": "PAZ- Analyzer Stereo",
        "PluginFormat": 2
      },
      {
        "PluginName": "Voxengo Correlometer",
        "PluginFormat": 2
      },
      {
        "PluginName": "MMultiAnalyzer",
        "PluginFormat": 2
      },
      {
        "PluginName": "Sonarworks Reference 4 VST",
        "PluginFormat": 2
      }
    ]
